#include<stdio.h>

int s[10]={3,4,5,7};
int top=3,n=5,i;
int* push(){
    int ele,pos;
    printf("Enter the element:");
    scanf("%d",&ele);
        if (top==n-1){
        printf("Overflow");
    }
    else{
        s[top+1]=ele;
        top+=1;
    }
    return s;
}
int  pop(){
    int val;
    val=s[top]
    top=top-1;
    return val;
    
int peep(){
    printf("The topmost element is:%d",s[top]);
    return s[top];
}

int change(){
    int ele,pos;
    printf("Enter the element");
    scanf("%d",&ele);
    printf("Entr the position");
    scanf("%d",&pos);
    s[pos]=ele;
    return 0;
}

    
int main(){
    int ch;
    
    printf("1.Push\n2.Pop\n3.Peep\n4.Change\n");
    while(ch!=0){
    printf("\nEnter choice or 0 to exit:");
    scanf("%d",&ch);
    switch(ch){
    case 0: printf("Program exiting.....");
    break;    
    case 1: push();
        break;
    case 2: pop();
        break;
    case 3: peep();
    break;
    case 4:change();
    break;
    default:printf("Please enter a correct choice:");
    
    }
    }
    return 0;
    
}
