//Queue operations
#include<stdio.h>
int q[10];
int f=-1,r=-1,n=10,i;
int enqueue(){
    int ele;
    printf("Enter the element:");
    scanf("%d",&ele);
        if(r==n-1){
            printf("Queue Overflow");}
        if(f==-1 && r==-1)
            f=r=0;
        else
            r=r+1;
        q[r]=ele;
}
int dequeue(){
    int val;
    if(f==-1)
        printf("Queue Underflow");
    else{
        val=q[f];
        if(f==r)
            f=r=-1;
        else
            f=f+1;
        printf("The deleted value is:%d", val);
    }
    
}

int traverse(){
    for(int i=f;i<=r;i++){
        printf("%d\t",q[i]);
    }
}
int main(){
    int ch;
    
    printf("1.Queue\n2.Enqueue\n3.Traverse");
    while(ch!=0){
    printf("\nEnter choice or 0 to exit:");
    scanf("%d",&ch);
    switch(ch){
    case 0: printf("Program exiting.....");
    break;    
    case 1: enqueue();
        break;
    case 2: dequeue();
    break;
    case 3:traverse();
    break;
    default:printf("Please enter a correct choice:");
    
    }
    }
    return 0;
    
}

